@model IEnumerable<MvcDemo1.Models.SysUser>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table>
        <tr>
            <th>@Html.DisplayNameFor(model=>model.Username)</th>
            <th>@Html.DisplayNameFor(model => model.Email)</th>
            <th></th>
        </tr>
        @foreach (var m in Model) { 
        <tr>
            <td>@Html.DisplayFor(ms=>m.Username)</td>
            <td>@Html.DisplayFor(ms => m.Email)</td>
            <td>@Html.ActionLink("Details", "Details",new { id=m.ID})</td>
        </tr>
        }
    </table>
</body>
</html>
